{
	"name": "Meet Attendance List",
	"version": "1.4",
	"description": "Monitor who attendant your class/meeting!",
	"background": {
		"scripts": ["js/listeners.js"],
		"persistent": false
	},
	"web_accessible_resources": ["js/inject.js"],
	"content_scripts": [
		{
			"matches": ["*://meet.google.com/**-**-**"],
			"js": ["js/3D party/jquery-3.6.0.min.js", "js/contentScript.js", "js/classButtons.js", "js/tagbox-logic.js", "js/html-templates.js", "js/translations.js"],
			"css": ["css/style.css", "css/pro.min.css"]
		}
	],
	"permissions": ["storage", "tabs"],
	"browser_action": {
		"default_icon": "img/image.png",
		"default_popup": "html/popup.html"
	},
	"manifest_version": 2
}